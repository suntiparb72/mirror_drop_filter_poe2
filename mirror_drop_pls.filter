# !! Waypoint c0.start : "Start - Override ALL rules"

Show # %D6 $type->gold $tier->stack3 !gold_pilehuge
	StackSize >= 5000
	BaseType == "Gold"
	SetFontSize 40
	SetTextColor 255 255 255 255
	SetBorderColor 255 255 255 255
	SetBackgroundColor 20 20 0 255
	PlayEffect Orange
	MinimapIcon 1 Yellow Cross

